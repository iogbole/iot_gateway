<p id="notice"><%= notice %></p>

<h1>Listing Dhts</h1>

<table>
  <thead>
    <tr>
      <th>Chipid</th>
      <th>Location</th>
      <th>Description</th>
      <th>Temperature</th>
      <th>Humidity</th>
      <th>Heat Index</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @dhts.each do |dht| %>
      <tr>
        <td><%= dht.chipid %></td>
        <td><%= dht.location %></td>
        <td><%= dht.description %></td>
        <td><%= dht.temperature %></td>
        <td><%= dht.humidity %></td>
          <td><%= dht.heat_index %></td>
        <td><%= link_to 'Show', dht %></td>
        <td><%= link_to 'Edit', edit_dht_path(dht) %></td>
        <td><%= link_to 'Destroy', dht, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dht', new_dht_path %>
